<h1>Editar</h1>
<%= render "form", noticia: @noticia %>
<br>
<div>
  <%= link_to "Mostrar", @noticia, class:"btn btn-outline-primary rounded-pill mb-3" %> 
  <%= link_to "Voltar", noticias_path, class:"btn btn-outline-primary  rounded-pill mb-3" %> 
  <%= button_to "Apagar", @noticia, method: :delete, class:"btn btn-outline-danger rounded-pill mb-3" %>
</div>
